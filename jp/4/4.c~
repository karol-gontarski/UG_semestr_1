/* Program ten wypisuje liczby z tablicy tabela[]
   w odwrotnej kolejno≈õci: 12, 6, 4, 2, 1 */
#include <stdio.h>
int main()
{

    int wynik, i;
    printf("Podaj liczbe: ");
    scanf("%d", &i);
    wynik = 1;
    while (i > 0) {
	wynik *= i;
	i--;
    }

    printf("%d\n", wynik);
    return 0;
}
